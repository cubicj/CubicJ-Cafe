{
  "name": "cubicj-cafe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node -r ./load-env.js ./node_modules/next/dist/bin/next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop cubicj-cafe",
    "pm2:restart": "pm2 restart cubicj-cafe",
    "pm2:reload": "pm2 reload cubicj-cafe",
    "pm2:delete": "pm2 delete cubicj-cafe",
    "pm2:logs": "pm2 logs cubicj-cafe",
    "pm2:monit": "pm2 monit",
    "validate-env": "node validate-env.js",
    "analyze": "cross-env ANALYZE=true npm run build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser npm run build",
    "build:production": "cross-env NODE_ENV=production npm run build",
    "build:standalone": "npm run build && npm run export",
    "size-check": "npx size-limit",
    "perf-check": "npm run build && npm run start &amp; sleep 5 && npx lighthouse http://localhost:3000 --only-categories=performance --chrome-flags=\"--headless\" --output=html --output-path=./lighthouse-report.html && pkill -f 'npm run start'",
    "prisma:generate": "node -r ./load-env.js ./node_modules/.bin/prisma generate",
    "prisma:migrate": "node -r ./load-env.js ./node_modules/.bin/prisma migrate dev",
    "migrate:prod": "node -r ./load-env.js ./node_modules/.bin/prisma migrate deploy",
    "prisma:studio": "node -r ./load-env.js ./node_modules/.bin/prisma studio",
    "prisma:push": "node -r ./load-env.js ./node_modules/.bin/prisma db push"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "discord.js": "^14.21.0",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "prisma": "^6.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-select": "^5.10.2",
    "tailwind-merge": "^3.3.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.6",
    "@playwright/test": "^1.54.2",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "critters": "^0.0.23",
    "cross-env": "^10.0.0",
    "depcheck": "^1.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "node-loader": "^2.1.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
